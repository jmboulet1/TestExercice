<DAT name="USCRIPT" xml:space='preserve'>operation exec
params
	string	pstrField		: OUT
endparams

pstrField = ""
if ($componentname($instanceparent) == "SMM_SCOE1260_TP1")
	info.DUMMY = $text(IC_EDIREL860)
elseif (($componentname($instanceparent) == "SMM_SCOE1010_TPA"))
	info.DUMMY = $text(IC_EDISO860)
else
	return (-1)
endif ;if ($componentname($instanceparent) == "SMM_SCOE1260_TP1")
$formtitle = $text(INFOEDI_S)
if ($language == "F")
	$valrep(field2.DUMMY) = "TH=TH (Mille)"
	$valrep(field3.DUMMY) = "PE=PE (Prix par unité)&uSEP;PN=PN (Prix par Dix)&uSEP;HP=HP (Prix par Cent)&uSEP;TP=TP (Prix par Mille)&uSEP;TT=TT (Prix par Dix mille)&uSEP;HT=HT (Prix par Cent mille)"
else
	$valrep(field2.DUMMY) = "TH=TH (Thousand)"
	$valrep(field3.DUMMY) = "PE=PE (Price per Each)&uSEP;PN=PN (Price per Ten)&uSEP;HP=HP (Price per Hundred)&uSEP;TP=TP (Price per Thousand)&uSEP;TT=TT (Price per Ten Thousand)&uSEP;HT=HT (Price per Hundred Thousand)"
endif ;if ($language == "F")

edit

if ($status == 9) ;OK
	putitem/id pstrField, "LINE", field1.DUMMY
	putitem/id pstrField, "UOMQTY", field2.DUMMY
	putitem/id pstrField, "UOMPRICE", field3.DUMMY
	return (0)
else
	return (-1)
endif ;if ($status == 9) ;OK



trigger accept
	if (field1.DUMMY == "")
		call X_MESSAGE ("EF_MANDATORYFLD")
		$prompt = field1.DUMMY
		return (-1)
	endif
	if (field2.DUMMY == "")
		call X_MESSAGE ("EF_MANDATORYFLD")
		$prompt = field2.DUMMY
		return (-1)
	endif
	if (field3.DUMMY == "")
		call X_MESSAGE ("EF_MANDATORYFLD")
		$prompt = field3.DUMMY
		return (-1)
	endif
done



trigger erase
erase
if ($status &lt;0)
	message "Erase error; see message frame"
	rollback
else
	if ($status = 1)
		message "Erase is not allowed"
	else
		message "Erase was successful"
		commit
	endif
endif



trigger store
store
if ($status &lt;0)
    message $text(1500)
    rollback
else
    if ($status = 1)
        message $text(1723)
    else
        commit
        if ($status &lt; 0)
            rollback
        else
            message $text(1805)
        endif
    endif
endif                                         



trigger retrieveSequential
retrieve
if ($status &lt;0)
	message "Retrieve sequential did not succeed; see message frame"
endif



trigger retrieve
retrieve
if ($status &lt;0)
	message "Retrieve did not succeed; see message frame"
endif



trigger clear
clear
